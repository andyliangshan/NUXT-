@import "./base.styl"

html, body {
  background: #fff;
}

.zan {
  width: 100%;

  .fansTitle {
    padding 10px
    background: #fafafa;
    margin: 0;

    .backtoPage {
      width 12px
      height 20px
      padding: 0;
      float left

      a {
        display: block;
        width: 100%;
        height: 100%;

        img {
          width 12px
